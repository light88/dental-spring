    <build>
        <defaultGoal>package</defaultGoal>

        <finalName>credit</finalName>

        <resources>

            <resource>
                <directory>src/main/resources</directory>
                <includes>
                    <include>*.xml</include>
                    <include>*.properties</include>
                </includes>
                <excludes>
                    <exclude>web.xml</exclude>
                    <exclude>webdefault.xml</exclude>
                    <exclude>application.wadl</exclude>
                </excludes>
                <filtering>true</filtering>
            </resource>
            <!--<resource>-->
            <!--<directory>src/main/resources/config</directory>-->
            <!--<includes>-->
            <!--<include>override.${profile.id}.properties</include>-->
            <!--</includes>-->
            <!--</resource>-->
            <resource>
                <directory>src/main/resources</directory>
                <includes>
                    <include>web.xml</include>
                </includes>
                <targetPath>${project.build.directory}/${project.artifactId}/WEB-INF/</targetPath>
                <filtering>true</filtering>
            </resource>

            <resource>
                <directory>src/main/resources</directory>
                <includes>
                    <include>application.wadl</include>
                </includes>
                <targetPath>${project.build.directory}/${project.artifactId}/</targetPath>
                <filtering>true</filtering>
            </resource>

        </resources>

        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-war-plugin</artifactId>
                <configuration>
                    <webXml>${project.build.directory}/${project.artifactId}/WEB-INF/web.xml</webXml>
                    <webResources>
                        <resource>
                            <directory>${project.build.directory}/${project.artifactId}/</directory>
                            <includes>
                                <include>**/application.wadl</include>
                            </includes>
                        </resource>
                    </webResources>
                    <archiveClasses>true</archiveClasses>
                </configuration>
            </plugin>

            <!-- set system properties -->
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>properties-maven-plugin</artifactId>
                <executions>
                    <execution>
                        <goals>
                            <goal>set-system-properties</goal>
                        </goals>
                        <configuration>
                            <properties>
                                <property>
                                    <name>logback.configurationFile</name>
                                    <value>${project.build.directory}/classes/logback.xml</value>
                                </property>
                            </properties>
                        </configuration>
                    </execution>
                </executions>
            </plugin>

            <plugin>
                <groupId>org.mortbay.jetty</groupId>
                <artifactId>maven-jetty-plugin</artifactId>
                <configuration>
                    <scanIntervalSeconds>10</scanIntervalSeconds>
                    <!-- <contextPath>/${project.artifactId}</contextPath> -->
                    <contextPath>/</contextPath>
                    <!--<Set>true</Set>-->
                    <webAppConfig>
                        <contextPath>/</contextPath>
                        <baseResource implementation="org.mortbay.resource.ResourceCollection">
                            <resourcesAsCSV>
                                src/main/webapp,
                                ${project.build.directory}/${project.artifactId}/
                            </resourcesAsCSV>
                        </baseResource>
                    </webAppConfig>
                    <overrideWebXml>src/main/resources/jetty-override-web.xml</overrideWebXml>
                    <webXml>${project.build.directory}/${project.artifactId}/WEB-INF/web.xml</webXml>
                    <systemProperties>
                        <systemProperty>
                            <name>jetty.port</name>
                            <value>8888</value>
                        </systemProperty>
                    </systemProperties>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.flywaydb</groupId>
                <artifactId>flyway-maven-plugin</artifactId>
                <version>3.1</version>
                <configuration>
                    <table>SchemaVersion</table>
                    <!--<driver>${db.driver}</driver>-->
                    <!--<url>${db.url}</url>-->
                    <!--<user>${db.username}</user>-->
                    <!--<password>${db.password}</password>-->
                    <schemas>
                        <schema>dbo</schema>
                    </schemas>
                    <locations>
                        <location>filesystem:${basedir}/doc/sql/migration</location>
                    </locations>
                    <initOnMigrate>true</initOnMigrate>
                </configuration>
            </plugin>

